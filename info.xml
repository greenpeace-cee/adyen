<?xml version="1.0"?>
<extension key="adyen" type="module">
  <file>adyen</file>
  <name>Adyen payment processor</name>
  <description>Payment processor for adyen.com</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Matthew Wire</author>
    <email>mjw@mjwconsult.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/mjwconsult/civicrm-adyen</url>
    <url desc="Support">https://mjw.pt/support/adyen</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-05-15</releaseDate>
  <version>0.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.51</ver>
  </compatibility>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Adyen</namespace>
    <format>22.05.2</format>
  </civix>
  <requires>
    <ext>mjwshared</ext>
  </requires>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
